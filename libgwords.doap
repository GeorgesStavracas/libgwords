  <name xml:lang="en">libgwords</name>
  <shortdesc xml:lang="en">A GObject-based word handling library</shortdesc>
  <description>libgwords is a GObject-based library to handle words, dictionaries
    and text structures, designed to be extended to allow high-quality natural
    language text processing.</description>
  <homepage rdf:resource="https://github.com/GeorgesStavracas/libgwords" />
  <download-page rdf:resource="https://github.com/GeorgesStavracas/libgwords/releases" />
  <bug-database rdf:resource="https://github.com/GeorgesStavracas/libgwords/issues" />
  <category rdf:resource="http://api.gnome.org/doap-extensions#core" />
  <programming-language>C</programming-language>

